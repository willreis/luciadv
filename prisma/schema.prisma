generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}


datasource db {
  provider = "sqlite"  // Ou MySQL, PostgreSQL...
  url      = "file:./dev.db"
}

model Contato {
  id        Int     @id @default(autoincrement())
  nome      String
  email     String
  telefone  String
  mensagem  String
  criadoEm  DateTime @default(now())
}
